<theme>
<formatVersion>4</formatVersion>


<view name="system">
	<carousel name="systemcarousel">
		<type>vertical</type>
		<pos>0 0</pos>
		<size>1 1</size>
		<origin>0 0</origin>
		<color>00000000</color>
